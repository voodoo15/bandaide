<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>


  <%= form_tag(user_registration_path,:method => 'post') %>
  <%= devise_error_messages! %>
  <table>

  <tr>
  <td>Username : </td>
  <td><%=text_field_tag "user[username]",@user.username %></td>
  </tr>

  <tr>
  <td>Email : </td>
  <td><%=email_field_tag "user[email]",@user.email %></td>
  </tr>

  <tr>
  <td>Password : </td>
  <td><%=password_field_tag "user[password]",@user.password %></td>
  </tr>

  <tr>
  <td>Retype password : </td>
  <td><%=password_field_tag "user[password_confirmation]",@user.password_confirmation %></td>
  </tr>

  <tr>
  <td>First Name  : </td>
  <td><%=text_field_tag "first_name[musician]" ,@musician.first_name %></td>
  </tr>

  <tr>
  <td>Last Name  : </td>
  <td><%=text_field_tag "last_name[musician]" ,@musician.last_name %></td>
  </tr>

  <tr>
  <td> City : </td>
  <td><%=text_field_tag "city[musician]" ,@musician.city %></td>
  </tr>

  <tr>
  <td> Province  : </td>
  <td><%=text_field_tag "province[musician]" ,@musician.province %></td>
  </tr>

  <tr>
  <td> Address : </td>
  <td><%=text_field_tag "province[address]" ,@musician.address %></td>
  </tr>

  <tr>
  <td> Postal Code : </td>
  <td><%=text_field_tag "province[postal_code]" ,@musician.postal_code %></td>
  </tr>

  <tr>
  <td><%= submit_tag nil, :class => "form_submit"%></td>
  </tr>
  </table>
  <br />
  <%= render :partial => "users/shared/links" %>
<%end %>
